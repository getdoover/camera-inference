{
    "camera_inference": {
        "name": "camera_inference",
        "display_name": "Camera Inference",
        "type": "DEV",
        "visibility": "PUB",
        "allow_many": true,
        "description": "A Doover App to provide object inference and detection from an RTSP stream",
        "long_description": "README.md",
        "depends_on": [],
        "owner_org_key": "36a22e32-ab81-4a39-b400-869d532a052d",
        "image_name": "ghcr.io/getdoover/camera_inference:main",
        "container_registry_profile_key": "88953909-e9c4-4f81-9ec0-56239d4b0a2e",
        "build_args": "--platform linux/amd64,linux/arm64",
        "config_schema": {
            "$schema": "https://json-schema.org/draft/2020-12/schema",
            "$id": "",
            "title": "Application Config",
            "type": "object",
            "properties": {
                "model": {
                    "title": "Model",
                    "x-name": "model",
                    "x-hidden": false,
                    "type": "string",
                    "description": "Model to use for inference. Common choices are yolo11n, yolov8n and yolov8n-pose",
                    "default": "yolo11n"
                },
                "rtsp_uri": {
                    "title": "RTSP URI",
                    "x-name": "rtsp_uri",
                    "x-hidden": false,
                    "type": "string",
                    "description": "RTSP URI of the camera to analyse"
                },
                "resize_width": {
                    "title": "Resize Width",
                    "x-name": "resize_width",
                    "x-hidden": false,
                    "type": "integer",
                    "description": "Width to resize the camera feed to. 0 to disable.",
                    "default": 0
                },
                "viewer_port": {
                    "title": "Viewer Port",
                    "x-name": "viewer_port",
                    "x-hidden": false,
                    "type": "integer",
                    "description": "Port to serve the live viewer on. 0 to disable.",
                    "default": 8080
                },
                "confidence_threshold": {
                    "title": "Confidence Threshold",
                    "x-name": "confidence_threshold",
                    "x-hidden": false,
                    "type": "integer",
                    "description": "The confidence threshold for detection",
                    "default": 70,
                    "minimum": 1,
                    "maximum": 100
                },
                "object_whitelist": {
                    "title": "Object Whitelist",
                    "x-name": "object_whitelist",
                    "x-hidden": false,
                    "type": "array",
                    "description": "List of object names to track. Empty to track all.",
                    "items": {
